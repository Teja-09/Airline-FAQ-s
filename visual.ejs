<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <script src="https://cdn.anychart.com/js/8.0.1/anychart-core.min.js"></script>
    <script src="https://cdn.anychart.com/js/8.0.1/anychart-pie.min.js"></script>
    <title>JS pie chart</title>
    <style>
        html, body, #container 
        {
        width: 100%;
        height: 100%;
        margin: 0;
        padding: 0;
        }
    </style>
</head>
<body>
    <div id="container"></div>
    <script>
        //chart code here.
        anychart.onDocumentReady(function() {
        // set the data
        var baggage = <%= bagagge %>;
        var license = <%= license %>;
        var passenger = <%= passenger %>;
        var security = <%= security %>;
        var spl = <%= spl %>;
        console.log
        var data = [
            {x: "Baggage", value: baggage},
            {x: "Security", value: security},
            {x: "Passenger", value: passenger},
            {x: "Special Needs", value:spl},
            {x: "License", value: license},
        ];

        // create the chart
        var chart = anychart.pie();

        // set the chart title
        chart.title("Visualisation of Queries on different categoies");

        // add the data
        chart.data(data);

        // display the chart in the container
        chart.container('container');
        chart.draw();

        });
    </script>
</body>
</html>